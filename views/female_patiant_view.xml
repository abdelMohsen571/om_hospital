<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <record id="action_female_patiant_hospital" model="ir.actions.act_window">
        <field name="name">Female Patiant</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hospital.patiant</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{'default_gender':'female','default_age':'26'}</field>
        <field name="domain">[('gender','=','female')]</field>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                Create your first pationt!
            </p>
        </field>
    </record>

    <menuitem id="female_patiant_menu"
              name="Female Patiants"
              sequence="0"
              parent="patiant_root_menu"
              action="action_female_patiant_hospital"/>
</odoo>